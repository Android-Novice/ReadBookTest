<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <color android:color="@android:color/holo_green_light"></color>
    </item>
    <item android:state_pressed="false">
        <color android:color="@android:color/holo_blue_bright"></color>
    </item>
    <item android:state_selected="true">
        <color android:color="@android:color/holo_purple"></color>
    </item>
    <item android:state_selected="false">
        <color android:color="@android:color/holo_purple"></color>
    </item>
    <item android:state_focused="true">
        <color android:color="@android:color/holo_orange_light"/>
    </item>
    <item android:state_focused="false">
        <color android:color="@android:color/holo_orange_light"/>
    </item>
    <item android:state_checked="true">
        <color android:color="@android:color/holo_red_light"></color>
    </item>
    <item android:state_checked="false">
        <color android:color="@android:color/holo_red_light"></color>
    </item>
</selector>